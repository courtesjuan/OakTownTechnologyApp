{
  "version": 1,
  "routes": [
    { "handle": "filesystem" },

    {
      "src": "^/adminportal/static/(.*)$",
      "dest": "/static/$1"
    },
    {
      "src": "^/adminportal/(favicon\\.ico|manifest\\.json)$",
      "dest": "/$1"
    },

    { "src": "^/adminportal$",    "dest": "/index.html" },
    { "src": "^/adminportal/(.*)$","dest": "/index.html" }
  ]
}
